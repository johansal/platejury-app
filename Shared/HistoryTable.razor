<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Track</th>
                <th scope="col">Added by</th>
                <th scope="col">Total pts.</th>
                <th scope="col">#</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var h in History) 
            {
                <tr>
                    <td>@h.TrackName</td>
                    <td>@h.UserName</td>
                    <td>@h.Points</td>
                    <td>@h.Position</td>
                </tr>
            }
        </tbody>
    </table>
</div>

@code
{
    [Parameter]
    public IEnumerable<Data.HistoryTrack> History {get; set;} = [];
}